  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#84563B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation Comments",
      "scope": [
        "comment.block.documentation",
        "comment.line.documentation"
      ],
      "settings": {
        "foreground": "#9BB589",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#79966d"
      }
    },
    {
      "name": "String Escapes",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#D3BE47"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language.numeric"
      ],
      "settings": {
        "foreground": "#D3BE47"
      }
    },
    {
      "name": "Booleans",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#F67422"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#F67422"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#F67422"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.relational"
      ],
      "settings": {
        "foreground": "#5B7C99"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#E9A131"
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#F4DDB3"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "storage.type.primitive"
      ],
      "settings": {
        "foreground": "#D3BE47"
      }
    },
    {
      "name": "Constructors",
      "scope": [
        "entity.name.function.constructor",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#E9A131"
      }
    },
    {
      "name": "Attributes and Decorators",
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#E9A131"
      }
    },
    {
      "name": "Properties and Fields",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#5B7C99"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#F4DDB3"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#F67422"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#84563B"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.block",
        "punctuation.section.block",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "punctuation.section.brackets"
      ],
      "settings": {
        "foreground": "#84563B"
      }
    },
    {
      "name": "Tags (HTML, XML)",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#E9A131"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#E9A131"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#E9A131"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#5B7C99"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "string.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#5B7C99"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#E9A131",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#F67422",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#F67422",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#79966d"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#5B7C99",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#79966d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#79966d"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E84A38"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#D3BE47"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#D3BE47",
    "class": "#D3BE47",
    "enum": "#D3BE47",
    "interface": "#D3BE47",
    "struct": "#D3BE47",
    "typeParameter": "#D3BE47",
    "type": "#D3BE47",
    "parameter": "#F4DDB3",
    "variable": "#F4DDB3",
    "property": "#5B7C99",
    "enumMember": "#F67422",
    "decorator": "#E9A131",
    "event": "#E9A131",
    "function": "#E9A131",
    "method": "#E9A131",
    "macro": "#F67422",
    "label": "#E9A131",
    "comment": "#84563B",
    "string": "#79966d",
    "keyword": "#F67422",
    "number": "#D3BE47",
    "regexp": "#79966d",
    "operator": "#5B7C99"
  }
}
